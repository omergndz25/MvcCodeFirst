@using webUygulamasi.Models.Siniflar
@model List<Personel>
@{
    ViewBag.Title = "StaffDetail";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<br />
<h2>Personel Detay Sayfası</h2>
<br />

<div style="background-color:darkgray">




</div>

<br />

<table class="table table-hover">


    <tr>
    
        <th>PERSONEL İLETİŞİM NUMARASI</th>
        <th>PERSONEL ADRES BİLGİSİ</th>
        <th>PERSONEL İŞE GİRİŞ TARİHİ</th>
        <th>PERSONELE VERİLEN ÜCRET</th>
    </tr>

    @foreach (var c in Model)
    {

<tr>
   
    <td>
        @c.PersonelTELEFON

    </td>
    <td>
        @c.PersonelADRES

    </td>


    <td>
        @c.PersonelGİRİSTARİHİ
    </td>
    <td>
        @c.PersonelUCRET
    </td>





</tr>
    }

</table>
<a href="/Personel/StaffView/" class="btn btn-info"> Tüm Personelleri Görüntüle</a>